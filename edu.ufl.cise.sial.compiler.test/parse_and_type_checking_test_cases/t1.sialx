import "aces_defs.sialx"

sial t1
index i = 1,30
index j = 30,50

distributed a[i,j]
distributed b[j,i]
temp c[i,j]
temp d[i,j]
scalar x 

proc F
do i
do j
get a [i,j]
get b [j, i]
x = a[i,j] * b[j,i]
print_scalar x
enddo j

enddo i 
endproc F

call F
execute blocks_to_list a

endsial t1