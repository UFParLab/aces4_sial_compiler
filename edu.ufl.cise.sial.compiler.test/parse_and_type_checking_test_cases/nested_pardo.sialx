import "aces_defs.sialx"

sial nested_pardo

aoindex a = 1,norb
aoindex b = 1,norb
aoindex ccc = 1,norb

distributed x[a,b]
distributed y[b,ccc]
temp t[a,ccc]

pardo a,b
   get x[a,b]
   pardo ccc
     get y[b,ccc]
     t[a,ccc] = x[a,b] * y[b,ccc]
   endpardo ccc
endpardo a,b

endsial nested_pardo
   