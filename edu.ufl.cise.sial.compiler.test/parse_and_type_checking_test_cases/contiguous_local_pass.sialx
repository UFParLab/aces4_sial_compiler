sial contiguous_local_pass

predefined int norb
special f rw
aoindex i = 1:norb
aoindex j = 1:3
int a
contiguous local c[i,j]
local d[i,j]

allocate contiguous c[1:norb/2,1:3]
allocate contiguous c[norb/2+1:norb, 1:3]

pardo i,j
   d[i,j] = c[i:i, j:j]
   execute f d[i,j] c[1:norb/2,1:3]
   execute f d[i,j] c[norb/2+1:norb, 1:3]
endpardo i,j
   
endsial contiguous_local_pass